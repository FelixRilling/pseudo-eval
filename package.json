{
    "name": "pseudo-eval",
    "version": "5.4.0",
    "description": "A library to evaluate simple expression strings without actually calling eval()",
    "main": "dist/pseudoEval.common.js",
    "module": "dist/pseudoEval.esm.js",
    "constants": {
        "dirBase": {
            "input": "./src",
            "output": "./dist"
        },
        "js": {
            "input": "main",
            "namespace": {
                "module": "pseudoEval",
                "file": "pseudoEval"
            },
            "targets": {
                "browsers": "chrome >= 58"
            }
        }
    },
    "scripts": {
        "start": "npm-run-all lint build test",
        "build": "node tasks/jsDev.js",
        "dist": "node tasks/jsDist.js",
        "lint": "eslint src/",
        "test": "jasmine"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/FelixRilling/pseudo-eval.git"
    },
    "keywords": [
        "eval",
        "expression"
    ],
    "author": "Felix Rilling",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/FelixRilling/pseudo-eval/issues"
    },
    "homepage": "https://github.com/FelixRilling/pseudo-eval#readme",
    "devDependencies": {
        "babel-core": "^7.0.0-beta.3",
        "babel-plugin-external-helpers": "^7.0.0-beta.3",
        "babel-preset-env": "^1.6.1",
        "eslint": "^4.9.0",
        "eslint-plugin-unicorn": "^2.1.2",
        "jasmine": "^2.8.0",
        "npm-run-all": "^4.1.1",
        "rollup": "^0.50.0",
        "rollup-plugin-babel": "^3.0.2",
        "rollup-plugin-node-resolve": "^3.0.0",
        "rollup-plugin-uglify-es": "0.0.1",
        "uglify-es": "^3.1.3"
    },
    "dependencies": {
        "lightdash": "^2.5.4"
    }
}
